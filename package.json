{
  "name": "vscode-workspace-switcher",
  "displayName": "vscode-workspace-switcher",
  "description": "Easily switch between workspaces",
  "version": "1.10.3",
  "publisher": "sadesyllas",
  "homepage": "https://github.com/sadesyllas/vscode-workspace-switcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/sadesyllas/vscode-workspace-switcher.git"
  },
  "bugs": {
    "url": "https://github.com/sadesyllas/vscode-workspace-switcher/issues"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "workspace",
    "save",
    "switch"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:extension.saveWorkspace",
    "onCommand:extension.switchWorkspace",
    "onCommand:extension.switchWorkspaceNewWindow"
  ],
  "main": "./dist/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "vscode-workspace-switcher configuration",
      "properties": {
        "vscodeWorkspaceSwitcher.paths": {
          "type": "array",
          "default": [],
          "description": "Paths of directories when `.code-workspace` files can be saved and then read from"
        },
        "vscodeWorkspaceSwitcher.codeExecutable": {
          "type": "string",
          "default": "code",
          "description": "Path to the executable file for VS Code"
        },
        "vscodeWorkspaceSwitcher.codeInsidersExecutable": {
          "type": "string",
          "default": "code-insiders",
          "description": "Path to the executable file for VS Code Insiders"
        }
      }
    },
    "commands": [
      {
        "command": "extension.saveWorkspace",
        "title": "Save workspace..."
      },
      {
        "command": "extension.switchWorkspace",
        "title": "Switch workspace..."
      },
      {
        "command": "extension.switchWorkspaceNewWindow",
        "title": "Switch workspace in new window..."
      }
    ],
    "keybindings": [
      {
        "key": "Ctrl-k Shift-w",
        "command": "extension.saveWorkspace"
      },
      {
        "key": "Ctrl-k w",
        "command": "extension.switchWorkspace"
      },
      {
        "key": "Ctrl-k Ctrl-w",
        "command": "extension.switchWorkspaceNewWindow"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "fast-glob": "^2.2.6",
    "mkdir-p": "0.0.7"
  }
}
